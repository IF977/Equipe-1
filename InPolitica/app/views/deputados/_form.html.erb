    <%= f.label :idade %><br>
    <%= f.number_field :idade %>
  </div>
  <div class="field">
    <%= f.label :url_foto %><br>
    <%= f.text_field :url_foto %>
  </div>
  <div class="field">
    <%= f.label :partido %><br>
    <%= collection_select(:deputado, :partido_id, Partido.all,:id,:nome) %>
  </div>
   <div class="field">
    <%= f.label :estado %><br>
    <%= collection_select(:deputado, :estado_id, Estado.all,:id,:nome) %>
