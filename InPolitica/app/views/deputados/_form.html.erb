<%= form_for(@deputado) do |f| %>
  <% if @deputado.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@deputado.errors.count, "error") %> prohibited this deputado from being saved:</h2>

      <ul>
      <% @deputado.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nome %><br>
